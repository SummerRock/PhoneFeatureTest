<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_gps_test"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yanxia.phonefeaturetest.testactivity.SVGTestActivity"
    tools:showIn="@layout/activity_svg_test">

    <TextView
        android:id="@+id/tv_gps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="60dp"
        android:text="@string/gpslocation" />

    <TextView
        android:id="@+id/phone_tv_gps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tv_gps"
        android:layout_toEndOf="@id/tv_gps"
        android:textColor="@android:color/black" />

</RelativeLayout>
