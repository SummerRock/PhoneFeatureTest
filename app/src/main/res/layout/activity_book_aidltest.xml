<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context=".multiProcess.BookAIDLTestActivity">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/new_book_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="新书上架"
        android:textColor="@android:color/black"
        android:textSize="26sp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="addBookInOut"
        android:text="addBookInOut" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="addBookIn"
        android:text="addBookIn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="addBookOut"
        android:text="addBookOut" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="printBookList"
        android:text="printBookList" />
</LinearLayout>